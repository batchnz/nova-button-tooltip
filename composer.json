{
    "name": "batchnz/nova-button-tooltip",
    "description": "A Laravel Nova Button Tooltip.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "authors": [
      {
        "name": "Matthias Law",
        "email": "matthias@batch.nz"
      }
    ],
    "autoload": {
        "psr-4": {
            "Batch\\ButtonTooltip\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Batch\\ButtonTooltip\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
